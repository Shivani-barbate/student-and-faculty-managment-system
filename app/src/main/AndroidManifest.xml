<?xml version="1.0" encoding="utf-8"?>
<!--The manifest file describes essential information about your app to the Android build tools,Android opertating system and also googleplay-->
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.stucher">
<uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/apklogo"
        android:label="Student And Faculty Information system"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">

        <activity android:name=".PsDetails"></activity>
        <activity android:name=".TsDetails" />
        <activity android:name=".Login" />
        <activity android:name=".Register" />
        <activity android:name=".MainActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>